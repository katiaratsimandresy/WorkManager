<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="{i18n>oData.FuncLoc.Funcloc}"
		contentWidth="70%" showHeader="true" horizontalScrolling="false" draggable="true"
		class="sapUiPopupWithPadding">

		<content>
			<Table id="funcLocationTable" growingThreshold="1000">
				<headerToolbar>
				</headerToolbar>
				<columns>
					<Column hAlign="Center" vAlign="Middle">
						<header>
							<ObjectIdentifier title="" />
						</header>
					</Column>
					<Column hAlign="Center" vAlign="Middle" width="30%">
						<header>
							<ObjectIdentifier
								title="{i18n>oData.FuncLoc.Functlocation}" />
						</header>
					</Column>
					<Column hAlign="Center" vAlign="Middle" width="40%">
						<header>
							<ObjectIdentifier
								title="{i18n>oData.FuncLoc.Descript}" />
						</header>
					</Column>
				</columns>
				
				<!-- Items will be generated at selection   -->
				
			</Table>
		</content>

		<endButton>
			<Button
				text="{i18n>createNotification.frag.button.cancel}"
				press="closeDialog" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>